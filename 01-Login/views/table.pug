extends layout

block content
  .w3-container
    if locals.user
      .container
      h1
        | DataTables Editor 
        span Usuarios
      table#users.table.table-striped.table-bordered(cellpadding="0" cellspacing="0" border="0" width="100%")
        thead
          tr
            th nombre
            th correo
            th telefono
            th sexo
    else
      h4 You are not logged in! Please #[a(href="/login") Log In] to continue.
